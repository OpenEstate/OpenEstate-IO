    },
    "featuresOrientationSouth": {
      "$ref": "features.json#/featuresOrientationSouth"
    },
    "featuresOrientationWest": {
      "$ref": "features.json#/featuresOrientationWest"
    },
    "featuresOrientationEast": {
      "$ref": "features.json#/featuresOrientationEast"
    },
    "featuresParkingAvailable": {
