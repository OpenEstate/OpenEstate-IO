    "featuresWardrobes": {
      "$ref": "features.json#/featuresWardrobes"
    },
    "featuresWindowsLocation": {
      "$ref": "features.json#/featuresWindowsLocation"
    },
    "featuresHeatingType": {
      "$ref": "features.json#/featuresHeatingType"
    }
  }
}