    },
    "featuresGarden": {
      "$ref": "features.json#/featuresGarden"
    },
    "featuresHandicapAdaptedAccess": {
      "$ref": "features.json#/featuresHandicapAdaptedAccess"
    },
    "featuresHandicapAdaptedUse": {
