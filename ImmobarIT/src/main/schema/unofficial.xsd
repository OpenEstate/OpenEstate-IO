            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="RevenuetypeValue">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                The action of an object.
            </xsd:documentation>
            <xsd:documentation xml:lang="de">
                Die Vermarktungsart eines Objekts.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">
                        purchase / sale
                    </xsd:documentation>
                    <xsd:documentation xml:lang="de">
                        Kauf
                    </xsd:documentation>
                    <xsd:documentation xml:lang="it">
                        Compravendita
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">
                        rent
                    </xsd:documentation>
                    <xsd:documentation xml:lang="de">
