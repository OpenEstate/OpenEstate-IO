		<xs:enumeration value="Angolo Cottura" />
	</xs:restriction>
</xs:simpleType>

<!-- heating -->
<xs:simpleType name="heat">
	<xs:restriction base="xs:string">
		<xs:enumeration value="Assente" />
		<xs:enumeration value="Autonomo" />
		<xs:enumeration value="Centralizzato" />
		<xs:enumeration value="Teleriscaldamento" />
	</xs:restriction>
</xs:simpleType>

<!-- garden -->
<xs:simpleType name="garden">
	<xs:restriction base="xs:string">
		<xs:enumeration value="Privato" />
		<xs:enumeration value="Comune" />
		<xs:enumeration value="Nessuno" />
	</xs:restriction>
</xs:simpleType>

<!-- yesno  -->
<xs:simpleType name="yesno">
	<xs:restriction base="xs:string">
		<xs:pattern value="Y|N" />
	</xs:restriction>
</xs:simpleType>

<!-- yesonly  -->
<xs:simpleType name="yesonly">
    <xs:restriction base="xs:string">
        <xs:pattern value="YES|yes" />
    </xs:restriction>
</xs:simpleType>

<!-- yesnoready  -->
<xs:simpleType name="yesnoready">
	<xs:restriction base="xs:string">
      <xs:enumeration value="Yes" />
      <xs:enumeration value="No" />
      <xs:enumeration value="Ready" />
	</xs:restriction>
</xs:simpleType>

<!-- floor -->
<xs:complexType name="floor">
  <xs:simpleContent>
      <xs:extension base="nullInteger">
		<xs:attribute
			name="type">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Intermedio" />
					<xs:enumeration value="Interrato" />
					<xs:enumeration value="Controterra" />
					<xs:enumeration value="Seminterrato" />
					<xs:enumeration value="PianoTerra" />
					<xs:enumeration value="Rialzato" />
					<xs:enumeration value="PianoNobile" />
					<xs:enumeration value="Multipiano" />
					<xs:enumeration value="Ultimo" />
					<xs:enumeration value="Attico" />
					<xs:enumeration value="Edificio" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	  </xs:extension>
	</xs:simpleContent>
</xs:complexType>

<!-- rental -->
<xs:simpleType name="rental">
	<xs:restriction base="xs:string">
		<xs:enumeration value="3+2" />
		<xs:enumeration value="4+4" />
		<xs:enumeration value="6+6" />
		<xs:enumeration value="9+9" />
		<xs:enumeration value="Transitorio" />
		<xs:enumeration value="Concordato" />
		<xs:enumeration value="Libero" />
		<xs:enumeration value="Studenti" />
	</xs:restriction>
</xs:simpleType>

<!-- furniture -->
<xs:simpleType name="furniture">
	<xs:restriction base="xs:string">
		<xs:enumeration value="Arredato" />
		<xs:enumeration value="Non Arredato" />
		<xs:enumeration value="Parzialmente Arredato" />
	</xs:restriction>
</xs:simpleType>

<!-- version -->
<xs:simpleType name="version">
    <xs:restriction base="xs:string">
        <xs:enumeration value="1.5" />
        <xs:enumeration value="1.6" />
        <xs:enumeration value="2.0" />
        <xs:enumeration value="2.1" />
		<xs:enumeration value="2.2" />
		<xs:enumeration value="2.2.1" />
		<xs:enumeration value="2.4" />
		<xs:enumeration value="2.5" />
		<xs:enumeration value="2.6" />
    </xs:restriction>
</xs:simpleType>

<!-- latitude -->
<xs:simpleType name="latitudeType">
	<xs:restriction base="xs:decimal">
		<xs:minInclusive value="27.2" />
		<xs:maxInclusive value="71.1" />
	</xs:restriction>
</xs:simpleType>

<!-- longitude -->
<xs:simpleType name="longitudeType">
	<xs:restriction base="xs:decimal">
		<xs:minInclusive value="-31.2" />
		<xs:maxInclusive value="38.9" />
	</xs:restriction>
</xs:simpleType>

<!-- video -->
<xs:simpleType name="videoType">
	<xs:restriction base="xs:string">
		<xs:enumeration value="local" />
		<xs:enumeration value="remote" />
	</xs:restriction>
</xs:simpleType>

<!-- projects property-type -->
<xs:simpleType name="propertyTypeProject">
	<xs:restriction base="xs:string">
	    <xs:enumeration value="Attico" />
	    <xs:enumeration value="Appartamento" />
	    <xs:enumeration value="Negozio" />
	    <xs:enumeration value="Ufficio" />
	    <xs:enumeration value="Box Auto" />
	    <xs:enumeration value="Villa" />
	    <xs:enumeration value="Villetta" />
	    <xs:enumeration value="Capannone" />
	    <xs:enumeration value="Loft" />
	    <xs:enumeration value="Magazzino" />
	</xs:restriction>
</xs:simpleType>

<!-- project floor -->
<xs:complexType name="floorProject">
  <xs:simpleContent>
      <xs:extension base="nullInteger">
		<xs:attribute name="type">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Intermedio" />
					<xs:enumeration value="Interrato" />
					<xs:enumeration value="Seminterrato" />
					<xs:enumeration value="PianoTerra" />
					<xs:enumeration value="Rialzato" />
					<xs:enumeration value="PianoNobile" />
					<xs:enumeration value="Multipiano" />
					<xs:enumeration value="Ultimo" />
					<xs:enumeration value="Attico" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	  </xs:extension>
	</xs:simpleContent>
</xs:complexType>

<!-- project garage -->
<xs:complexType name="boxProject">
  <xs:simpleContent>
      <xs:extension base="nullInteger">
		<xs:attribute name="type">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="No" />
					<xs:enumeration value="Box" />
					<xs:enumeration value="PostoAuto" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	  </xs:extension>
  </xs:simpleContent>
</xs:complexType>

<!-- verbose picture, INCLUDE pictureExtended -->
<xs:complexType name="pictureVerbose">
    <xs:all>
       <xs:element name="max" type="xs:string" minOccurs="0" />
       <xs:element name="middle" type="xs:string" minOccurs="0" />
       <xs:element name="tiny" type="xs:string" minOccurs="0" />
    </xs:all>
    <xs:attribute name="position" type="xs:integer" />
    <xs:attribute name="url" type="xs:string" />
</xs:complexType>

<!-- project picture-url -->
<xs:complexType name="pictureProject">
  <xs:simpleContent>
    <xs:extension base="xs:string">
      <xs:attribute name="position" type="xs:integer" />
    </xs:extension>
  </xs:simpleContent>
</xs:complexType>

<!-- picture node, obosleto. Vedere pictureVerbose -->
<xs:complexType name="pictureExtended">
  <xs:simpleContent>
    <xs:extension base="pictureProject">
      <xs:attribute name="url" type="xs:string" />
    </xs:extension>
  </xs:simpleContent>
</xs:complexType>

<!-- project video -->
<xs:complexType name="videoProject">
  <xs:simpleContent>
    <xs:extension base="xs:string">
      <xs:attribute name="type" type="videoType" />
    </xs:extension>
  </xs:simpleContent>
</xs:complexType>

<!-- rooms -->
<xs:complexType name="roomsType">
    <xs:simpleContent>
    <xs:extension base="xs:integer">
      <xs:attribute name="literal-value" type="xs:string" />
    </xs:extension>
    </xs:simpleContent>
</xs:complexType>

<!-- gender -->
<xs:simpleType name="genderType">
	<xs:restriction base="xs:string">
		<xs:pattern value="DoesntMatter|Male|Female" />
	</xs:restriction>
</xs:simpleType>

<!-- energy-class -->
<xs:simpleType name="classEnergy">
	<xs:restriction base="xs:string">
	    <xs:enumeration value="A4" />
        <xs:enumeration value="A3" />
        <xs:enumeration value="A2" />
        <xs:enumeration value="A1" />
	    <xs:enumeration value="A+" />
	    <xs:enumeration value="A" />
	    <xs:enumeration value="B" />
	    <xs:enumeration value="C" />
	    <xs:enumeration value="D" />
	    <xs:enumeration value="E" />
	    <xs:enumeration value="F" />
	    <xs:enumeration value="G" />
	    <xs:enumeration value="esente" />
	    <xs:enumeration value="Non classificabile" />
	    <xs:enumeration value="NC" />
	</xs:restriction>
</xs:simpleType>

<!-- advert-visibility -->
<xs:simpleType name="visibility">
    <xs:restriction base="xs:string">
        <xs:enumeration value="basic" />
        <xs:enumeration value="premium" />
        <xs:enumeration value="top" />
        <xs:enumeration value="showcase" />
    </xs:restriction>
</xs:simpleType>

<!-- energy-performance -->
<xs:complexType name="classEnergyPerformance">
  <xs:simpleContent>
    <xs:extension base="xs:string">
      <xs:attribute name="certified">
	<xs:simpleType>
	  <xs:restriction base="xs:string">
	    <xs:pattern value="yes|no" />
	  </xs:restriction>
	</xs:simpleType>
      </xs:attribute>
      <xs:attribute name="unit" use="required">
	<xs:simpleType>
	  <xs:restriction base="xs:string">
	    <xs:pattern value="kWh/m2anno|kWh/m3anno" />
	  </xs:restriction>
	</xs:simpleType>
      </xs:attribute>
    </xs:extension>
  </xs:simpleContent>
</xs:complexType>

<!-- additional-costs -->
<xs:complexType name="additionalCostsType">
  <xs:simpleContent>
    <xs:extension base="xs:float">
      <xs:attribute name="currency" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="[a-zA-Z]{3}" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:extension>
  </xs:simpleContent>
</xs:complexType>

<!-- begin XSD root element definition-->
<xs:element name="feed">
  <xs:complexType>
    <xs:all>
      <xs:element name="version" type="version" minOccurs="0" />
      <xs:element name="metadata" minOccurs="0">
        <xs:complexType>
          <xs:all>
            <xs:element name="publisher" type="publisher" />
            <xs:element name="build-date" type="xs:dateTime"/>
            <xs:element name="multipage" minOccurs="0">
              <xs:complexType>
                <xs:all>
                  <xs:element name="current" type="xs:integer"/>
                  <xs:element name="last" type="xs:integer"/>
                </xs:all>
              </xs:complexType>
            </xs:element>
          </xs:all>
        </xs:complexType>
      </xs:element>  
      <!-- begin projects section -->
      <xs:element name="projects" minOccurs="0">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
      	<xs:element name="project">
	      <xs:complexType>
		<xs:all>
		  <!-- <xs:element name="unique-id" type="xs:string" minOccurs="1"/> -->
		  
		  <xs:element name="unique-id" type="xs:string">
		    <xs:unique name="unique-id">
		      <xs:selector xpath="projects/project" />
		      <xs:field xpath="unique-id" />
		    </xs:unique>
		  </xs:element>

		  <xs:element name="published-on" type="dateUpdatedType" minOccurs="0" />
		  <xs:element name="date-updated" type="dateUpdatedType" />
		  <xs:element name="agent">
		    <xs:complexType>
		      <xs:all>
			<xs:element name="office-name" type="xs:string" minOccurs="1"/>
			<xs:element name="email" type="emailType" minOccurs="1"/>
			<xs:element name="phone" type="xs:string" minOccurs="0" />
			<xs:element name="mobile-phone" type="xs:string" minOccurs="0" />
			<xs:element name="vat" type="xs:string" minOccurs="0" />
			<xs:element name="builder" type="yesno" minOccurs="0" />
			<xs:element name="contact" minOccurs="1">
              <xs:complexType>
                <xs:all>
                  <xs:element name="firstname" type="xs:string" minOccurs="0" />
                  <xs:element name="surname" type="xs:string" minOccurs="0" />
                  <xs:element name="phone" type="xs:string" minOccurs="0" />
                </xs:all>
              </xs:complexType>
            </xs:element>
			<xs:element name="description" type="langtxt" minOccurs="0" />
			<xs:element name="location" type="locationStructure" />
            <xs:element name="logo" type="xs:string" minOccurs="0" />
			<xs:element name="sales-office" minOccurs="1">
			  <xs:complexType>
			    <xs:all>
			      <xs:element name="country-code" minOccurs="0">
				<xs:simpleType>
				  <xs:restriction base="xs:string">
				    <xs:pattern value="[a-zA-Z]{2}" />
				  </xs:restriction>
				</xs:simpleType>
			      </xs:element>
			      <xs:element name="administrative-area" type="xs:string" minOccurs="0"/>
			      <xs:element name="sub-administrative-area" type="nullString" minOccurs="0"/>
			      <xs:element name="city" minOccurs="0">
				<xs:complexType>
				  <xs:simpleContent>
				    <xs:extension base="xs:string">
				      <xs:attribute name="code" type="xs:integer" />
				    </xs:extension>
				  </xs:simpleContent>
				</xs:complexType>
			      </xs:element>
			      <xs:element name="locality">
				<xs:complexType>
				  <xs:sequence>
				    <xs:element name="thoroughfare" type="xs:string" minOccurs="1"/>
				  </xs:sequence>
				</xs:complexType>
			      </xs:element>
			      <xs:element name="phone" type="xs:string" minOccurs="0"/>
			      <xs:element name="mobile" type="xs:string" minOccurs="0"/>
			    </xs:all>
			  </xs:complexType>
			</xs:element>
		      </xs:all>
		      <xs:attribute name="id" type="xs:integer" />
		    </xs:complexType>
		  </xs:element>
		  
		  <xs:element name="features">
		    <xs:complexType>
		      <xs:sequence>
			<xs:element name="building-status" type="xs:string" minOccurs="0" />
			<xs:element name="start-date" type="xs:dateTime" minOccurs="0" />
			<xs:element name="end-date" type="xs:dateTime" minOccurs="0" />
			<xs:element name="name" type="xs:string" minOccurs="0" />
			<xs:element name="specification" type="xs:string" minOccurs="0" />
			<xs:element name="floorplanner-url" type="xs:string" minOccurs="0" />
			<xs:element name="virtual-tour" type="xs:string" minOccurs="0" />
			<xs:element name="description" type="langtxt" minOccurs="0" maxOccurs="25" />
			<xs:element name="specification" type="xs:string" minOccurs="0" />
			<xs:element name="energy-class" type="classEnergy" minOccurs="0" />
			<xs:element name="energy-performance" type="classEnergyPerformance" minOccurs="0" />
			<xs:element name="advert-visibility" type="visibility" minOccurs="0" />
		      </xs:sequence>
		    </xs:complexType>
		  </xs:element>
		  
		  <xs:element name="location" type="locationStructure" minOccurs="0" maxOccurs="1"/>
		  
		  <xs:element name="pictures" minOccurs="0">
		    <xs:complexType>
		      <xs:sequence>
		         <xs:element name="picture" type="pictureVerbose" maxOccurs="unbounded" minOccurs="0" />
			     <xs:element name="picture-url" type="pictureProject" maxOccurs="unbounded" minOccurs="0" />
		      </xs:sequence>
		    </xs:complexType>
		  </xs:element>
		  
		  <xs:element name="videos" minOccurs="0">
		    <xs:complexType>
		      <xs:all>
			<xs:element name="video" type="videoProject" />
		      </xs:all>
		    </xs:complexType>
		  </xs:element>
		  
		  <xs:element name="lots" minOccurs="0">
		    <xs:complexType>
		      <xs:sequence>
			<xs:element name="residential" type="xs:integer" />
			<xs:element name="commercial" type="xs:integer" />
			<xs:element name="lot" minOccurs="0" maxOccurs="unbounded">
			  <xs:complexType>
			    <xs:all>
			      <!--  <xs:element name="unique-id" type="xs:string" /> -->
			      <xs:element name="unique-id" type="xs:string" minOccurs="0">
					<xs:unique name="lot-unique-id">
					  <xs:selector xpath="lots/lot" />
					  <xs:field xpath="unique-id" />
					</xs:unique>
			      </xs:element>
			      <xs:element name="property-type" type="propertyTypeProject" />
			      <xs:element name="price" type="priceType" />
			      <xs:element name="size" type="sizeType" />
			      <xs:element name="rooms" type="roomsType" minOccurs="0" />
			      <xs:element name="bathrooms" type="roomsType" minOccurs="0" />
			      <xs:element name="floor" type="floorProject" minOccurs="0" />
			      <xs:element name="garage" type="boxProject" minOccurs="0" />
			      <xs:element name="description" type="langtxt" minOccurs="0" />
			      <xs:element name="specification" type="xs:string" minOccurs="0" />
			      <xs:element name="pictures" minOccurs="0">
				<xs:complexType>
				  <xs:sequence>
				    <xs:element name="picture" type="pictureVerbose" maxOccurs="unbounded" minOccurs="0" />
				    <xs:element name="picture-url" type="pictureProject" maxOccurs="unbounded" minOccurs="0" />
				  </xs:sequence>
				</xs:complexType>
			      </xs:element>
			    </xs:all>
			    <xs:attribute name="total" type="nullInteger" />
			    <xs:attribute name="forsale" type="nullInteger" />
			    <xs:attribute name="ID" type="xs:integer" />
			  </xs:complexType>
			</xs:element>
		      </xs:sequence>
		    </xs:complexType>
		  </xs:element>
		</xs:all>
		<xs:attribute name="operation" type="operation" />
		<xs:attribute name="ID" type="xs:integer" />
	      </xs:complexType>
	    </xs:element>
	  </xs:sequence>
      	</xs:complexType>
      </xs:element>
      <!-- end projects section -->
      
      <xs:element name="properties">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="property">
              <xs:complexType>
                <xs:all> <!-- mandatory fields -->
                  <xs:element name="unique-id" type="xs:string">
		    <xs:unique name="uniqueid">
		      <xs:selector xpath="properties/property" />
		      <xs:field xpath="unique-id" />
		    </xs:unique>
		  </xs:element>
		          <xs:element name="published-on" type="dateUpdatedType" minOccurs="0" />
                  <xs:element name="date-updated" type="dateUpdatedType" />
                  <xs:element name="date-expiration" minOccurs="0">
                    <xs:simpleType>
                      <xs:union memberTypes="xs:dateTime nullString" />
                    </xs:simpleType>
                  </xs:element>
                  <xs:element name="transaction-type" type="transactionType" />
		  <xs:element name="building" type="building" minOccurs="0" />
                  <xs:element name="category" type="cat" minOccurs="0" />
                  <xs:element name="property-type" type="proptype" minOccurs="0" />
                  <xs:element name="building-status" type="status" minOccurs="0" />
                  <xs:element name="agent">
                    <xs:complexType>
                      <xs:all>
                        <xs:element name="office-name" type="xs:string" minOccurs="0"/>
                        <xs:element name="email" type="emailType" />
                        <xs:element name="phone" type="xs:string" />
                        <xs:element name="mobile-phone" type="xs:string" minOccurs="0" />
                        <xs:element name="description" type="langtxt" minOccurs="0" />
                        <xs:element name="vat" type="xs:string" minOccurs="0" />
                        <xs:element name="web" type="xs:string" minOccurs="0" />
            <xs:element name="builder" type="yesno" minOccurs="0" />
            <xs:element name="contact" minOccurs="0">
              <xs:complexType>
                <xs:all>
                  <xs:element name="firstname" type="xs:string" minOccurs="0" />
                  <xs:element name="surname" type="xs:string" minOccurs="0" />
                  <xs:element name="phone" type="xs:string" minOccurs="0" />
                </xs:all>
              </xs:complexType>
            </xs:element>
                        <xs:element name="location" type="locationStructure" />
                        <xs:element name="logo" type="xs:string" minOccurs="0" />
                      </xs:all>
                      <xs:attribute name="id" type="xs:integer" />
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="location" type="locationStructure" />
                  <xs:element name="features">
