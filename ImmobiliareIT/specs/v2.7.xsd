                      					</xs:complexType>
                      				</xs:element>
    							</xs:all>
    						</xs:complexType>
    					</xs:element>
                      </xs:all>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="pictures" minOccurs="0">
                    <xs:complexType>
                      <xs:sequence minOccurs="0" maxOccurs="25">
                        <xs:element name="picture" type="pictureVerbose" maxOccurs="unbounded" minOccurs="0" />
                        <xs:element name="picture-url" type="pictureProject" maxOccurs="unbounded" minOccurs="0" />
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="blueprints" minOccurs="0">
				    <xs:complexType>
				      <xs:sequence>
				         <xs:element name="picture" type="pictureVerbose" maxOccurs="unbounded" minOccurs="0" />
					     <xs:element name="blueprint" type="pictureExtended" maxOccurs="unbounded" minOccurs="0" />
				      </xs:sequence>
				    </xs:complexType>
				  </xs:element>
           		  <xs:element name="videos" minOccurs="0">
           			<xs:complexType>
             			<xs:sequence minOccurs="1" maxOccurs="4">
               			<xs:element name="video" type="videoProject" />
             			</xs:sequence>
           			</xs:complexType>
         		  </xs:element>
                </xs:all>
                <xs:attribute name="operation" type="operation" />
                <xs:attribute name="ID" type="xs:integer" />
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>
</xs:element>

</xs:schema>
