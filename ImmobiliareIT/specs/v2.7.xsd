                        <xs:element name="energy-class" type="classEnergy" minOccurs="0" />
			<xs:element name="energy-performance" type="classEnergyPerformance" minOccurs="0" />
			            <xs:element name="advert-visibility" type="visibility" minOccurs="0" />
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="extra-features" minOccurs="0" maxOccurs="1">
                    <xs:complexType>
                      <xs:all>
